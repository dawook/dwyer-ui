.dr-alert {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -o-box-sizing: border-box;
  box-sizing: border-box;
  position: relative;
  display: inline-block;
  padding: 8px 12px;
  width: 100%;
  overflow: hidden;
  color: #657180;
  text-align: left;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  -o-border-radius: 5px;
  border-radius: 5px; }

.dr-alert--info {
  background-color: rgba(45, 183, 245, 0.15);
  border: 1px solid rgba(45, 183, 245, 0.4); }
  .dr-alert--info .dr-alert__icon {
    color: #2db7f5; }

.dr-alert--success {
  background-color: rgba(25, 213, 103, 0.15);
  border: 1px solid rgba(25, 213, 103, 0.4); }
  .dr-alert--success .dr-alert__icon {
    color: #19d567; }

.dr-alert--warning {
  background-color: rgba(255, 170, 0, 0.15);
  border: 1px solid rgba(255, 170, 0, 0.4); }
  .dr-alert--warning .dr-alert__icon {
    color: #fa0; }

.dr-alert--error {
  background-color: rgba(255, 85, 0, 0.15);
  border: 1px solid rgba(255, 85, 0, 0.4); }
  .dr-alert--error .dr-alert__icon {
    color: #f50; }

.dr-alert__icon {
  margin-top: 1px;
  font-size: 16px;
  display: table-cell;
  vertical-align: middle; }
  .dr-alert__icon.is-more-line {
    font-size: 30px; }
  .dr-alert__icon + .dr-alert__content {
    padding: 0 10px; }

.dr-alert__content {
  display: table-cell; }

.dr-alert__title {
  display: block;
  font-size: 12px;
  padding-top: 3px; }
  .dr-alert__title.is-more-line {
    font-size: 14px;
    color: #323e4d; }

.dr-alert__description {
  margin: 5px 0 0;
  font-size: 12px;
  line-height: 18px; }

.dr-alert__close {
  position: absolute;
